{
  "endpoint" : "getUserCurrent",
  "tests": [
    {
      "name" : "get current user",
      "categories": ["SMOKE"],
      "headers" : [
        {
          "name": "authorization",
          "value": "{{ constants.header_bearer_token }}"
        },
        {
          "name": "contentType",
          "value": "{{ constants.content_type }}"
        },
        {
          "name": "accept",
          "value": "true"
        }
      ],
      "actions": [
        {
          "methodName": "get",
          "url": "{{ env.url }}/GUI/user"
        }
      ],
      "assertions": [
        {
          "name": "status-code",
          "value": "200"
        }
      ]
    }
  ]
}
