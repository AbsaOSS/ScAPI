{
  "name" : "getUserCurrent",
  "tests": [
    {
      "name" : "get current user",
      "categories": ["SMOKE"],
      "headers" : [
        {
          "name": "authorization",
          "value": "{{ constants.header_basic_token }}"
        },
        {
          "name": "content-type",
          "value": "{{ constants.content_type }}"
        }
      ],
      "actions": [
        {
          "methodName": "get",
          "url": "{{ env.url }}/GUI/user"
        }
      ],
      "responseActions": [
        {
          "method": "assert.status-code-equals",
          "code": "200"
        },
        {
          "method": "assert.status-code-is-success"
        },
        {
          "method": "log.error",
          "message": "Dummy error log message."
        },
        {
          "method": "log.warn",
          "message": "Dummy warn log message."
        },
        {
          "method": "log.info",
          "message": "Dummy info log message."
        },
        {
          "method": "log.debug",
          "message": "Dummy debug log message."
        },
        {
          "method": "assert.response-time-is-below",
          "limit": "600000"
        },
        {
          "method": "assert.response-time-is-above",
          "limit": "1"
        },
        {
          "method": "assert.body-equals",
          "body": "TODO"
        },
        {
          "method": "assert.body-contains-text",
          "text": "TODO"
        },
        {
          "method": "assert.body-is-not-empty"
        },
        {
          "method": "assert.body-length-equals",
          "length": "1"
        },
        {
          "method": "assert.body-starts-with",
          "prefix": "testValue"
        },
        {
          "method": "assert.body-ends-with",
          "suffix": "testValue"
        },
        {
          "method": "assert.body-matches-regex",
          "regex": "testValue"
        },
        {
          "method": "assert.body-json-is-json-array"
        },
        {
          "method": "assert.body-json-path-exists",
          "jsonPath": "testValue"
        }
      ]
    }
  ]
}
